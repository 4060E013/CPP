
#include<iostream>
using namespace std;
int main()
{
	int i;
	char array[12];
	array[0]='w';
	array[1]='e';
	array[2]='l';
	array[3]='c';
	array[4]='o';
	array[5]='m';
	array[6]='e';
	printf("%s\n",array);
    return 0;
}

====================================

#include<iostream>
using namespace std;
int main()
{
	int i;
	char array[12]={'J','a','p','a','n',' ','N','i','n','j','a'};
	for(i=0;i<12;i++)
		cout<<array[i];
	cout << endl;
    return 0;
}

====================================

#include <iostream>
using namespace std;
int main()
{
	int i,a[30];

	for(i=0;i<30;i++)
		a[i]=i;

	for(i=0;i<30;i++)
		cout << a[i] << endl;
		
    return 0;
}


